---
import Nav from "../components/Nav.astro";
import Title from "../components/Title.astro";

const { title } = Astro.props;
---

<header
  id="landing-header"
  class="flex justify-between bg-white items-center py-4 px-10 fixed top-0 w-full shadow-md"
>
  <Title title={title} />

  <Nav />
</header>
